{
  "name": "powerbi-tom-parser",
  "version": "1.0.0",
  "description": "Node.js library to extract Power BI / Analysis Services Tabular Model metadata from .bim files",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "clean": "rm -rf dist",
