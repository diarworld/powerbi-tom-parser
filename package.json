  },
  "keywords": [
    "powerbi",
    "tabular",
    "analysis-services",
    "bim",
    "parser",
    "tom"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  }
}
